{
  "name": "dependency-tracer",
  "version": "0.1.0",
  "description": "A CLI tool for tracing, counting, and visualizing dependencies of Aptos Move packages.",
  "main": "dist/cli.ts",
  "types": "./dist/cli.d.ts",
  "type": "module",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tsc --noEmit src/*.ts",
    "build": "tsup src/cli.ts --format esm --minify --dts --out-dir dist",
    "dev": "tsx src/cli.ts"
  },
  "keywords": [
    "aptos",
    "move",
    "dependency",
    "tracer",
    "cli",
    "blockchain"
  ],
  "author": "Greg Nazario <greg@gnazar.io>",
  "license": "MIT",
  "dependencies": {
    "@aptos-labs/ts-sdk": "^3.1.3"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "pnpm": ">=10.13.1"
  }
}
